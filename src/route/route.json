[
  {
    "path": "/todos",
    "method": "post",
    "handler": "todo/addTodo.js"
  },
  {
    "path": "/todos",
    "method": "get",
    "handler": "todo/getTodoList.js"
  },
  {
    "path": "/todos/:todoId",
    "method": "get",
    "handler": "todo/getTodo.js"
  },
  {
    "path": "/todos/:todoId",
    "method": "put",
    "handler": "todo/putTodo.js"
  },
  {
    "path": "/todos/:todoId/complete",
    "method": "put",
    "handler": "todo/completeTodo.js"
  },
  {
    "path": "/todos/:todoId",
    "method": "delete",
    "handler": "todo/deleteTodo.js"
  },
  {
    "path": "/todos/:todoId/comments",
    "method": "post",
    "handler": "comment/addComment.js"
  },
  {
    "path": "/todos/:todoId/comments",
    "method": "get",
    "handler": "comment/getCommentList.js"
  },
  {
    "path": "/todos/:todoId/comments/:commentId",
    "method": "get",
    "handler": "comment/getComment.js"
  },
  {
    "path": "/todos/:todoId/comments/:commentId",
    "method": "put",
    "handler": "comment/putComment.js"
  },
  {
    "path": "/todos/:todoId/comments/:commentId",
    "method": "delete",
    "handler": "comment/deleteComment.js"
  }
]
